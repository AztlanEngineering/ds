    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-link-retry": "^2.2.15",
    "downshift": "^5.0.5",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "history": "^4.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.16",
    "stylelint": "^13.2.1",
    "stylelint-config-palantir": "^4.0.1",
    "stylelint-scss": "^3.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
